{
  "multipart": [
    {
      "apply": {
        "model": "powergrid:block/transformer/small"
      },
      "when": {
        "axis": "z"
      }
    },
    {
      "apply": {
        "model": "powergrid:block/transformer/small_coil1"
      },
      "when": {
        "axis": "z",
        "coils": "1|2"
      }
    },
    {
      "apply": {
        "model": "powergrid:block/transformer/small_coil2"
      },
      "when": {
        "axis": "z",
        "coils": "2"
      }
    },
    {
      "apply": {
        "model": "powergrid:block/transformer/small",
        "y": 90
      },
      "when": {
        "axis": "x"
      }
    },
    {
      "apply": {
        "model": "powergrid:block/transformer/small_coil1",
        "y": 90
      },
      "when": {
        "axis": "x",
        "coils": "1|2"
      }
    },
    {
      "apply": {
        "model": "powergrid:block/transformer/small_coil2",
        "y": 90
      },
      "when": {
        "axis": "x",
        "coils": "2"
      }
    }
  ]
}